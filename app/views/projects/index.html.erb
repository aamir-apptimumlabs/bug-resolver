<h1>Projects</h1>

<ul>
  <% @projects.each do |project| %>
    <li>
      <%= project.name %>
    </li>
  <% end %>
</ul>

<h3>New Project</h3>

<%= form_with model: @project do |form| %>
  <div>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  
  </div>

  <div>
    <%= form.label :description %><br>
    <%= form.text_area :description %>
   
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>

